{
  "name": "@trim21/php-serialize",
  "version": "0.0.5",
  "description": "PHP serialize/unserialize in Javascript",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.js",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest",
    "lint": "eslint --ext ts .",
    "format": "prettier -w ./",
    "build": "tsc",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "repository": "trim21/php-serialize",
  "author": "trim21",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trim21/php-serialize/issues"
  },
  "files": [
    "src/*",
    "lib/*"
  ],
  "lint-staged": {
    "*.{md,js,jsx,ts,tsx,json,yml,yaml}": [
      "prettier --ignore-path ./.prettierignore --write"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/trim21/php-serialize#readme",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "tsx": "^3.12.6",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^4.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  }
}

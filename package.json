{
  "name": "@trim21/php-serialize",
  "version": "0.1.1",
  "description": "PHP serialize/deserialize in Javascript",
  "main": "dist/index.cjs",
  "types": "dist/types/index.d.ts",
  "module": "dist/index.mjs",
  "sideEffects": false,
  "scripts": {
    "test": "NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest",
    "lint": "eslint --ext ts .",
    "format": "prettier -w ./",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "repository": "trim21/php-serialize",
  "author": "trim21",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trim21/php-serialize/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "package.json",
    "src/*",
    "README.md",
    "dist/*"
  ],
  "homepage": "https://github.com/trim21/php-serialize#readme",
  "dependencies": {},
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.2",
    "ts-jest": "^29.1.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "lint-staged": {
    "*.{md,js,cjs,mjs,jsx,ts,tsx,json,yml,yaml}": [
      "prettier --ignore-path ./.prettierignore --write"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
